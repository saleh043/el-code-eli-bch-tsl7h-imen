    <p id="average"></p>
    <script>
      function calculateAverage() {
        var control info = parseFloat(document.getElementById("control info").value);
        var test info = parseFloat(document.getElementById("test info").value);
        var test html = parseFloat(document.getElementById("test html").value);
        var synthese info = parseFloat(document.getElementById("synthese info").value);
        var moy info = ((test info + test html) / 2 + (control info + synthese info + synthese info)) /4;
        var score info = (moy info) *3;



        var control arabe = parseFloat(document.getElementById("control arabe").value);
        var oral arabe = parseFloat(document.getElementById("oral arabe").value);
        var synthese arabe = parseFloat(document.getElementById("synthese arabe").value);
        var moy arabe = (oral arabe + control arabe + synthese arabe + synthese arabe) /4;
        var score arabe = (moy arabe) *2;



        var control francais = parseFloat(document.getElementById("control francais").value);
        var oral francais = parseFloat(document.getElementById("oral francais").value);
        var synthese francais = parseFloat(document.getElementById("synthese francais").value);
        var moy francais = (control francais + oral francais + synthese francais + synthese francais) /4;
        var score francais = (moy francais) *2;



        var control anglais = parseFloat(document.getElementById("control anglais").value);
        var oral anglais = parseFloat(document.getElementById("oral anglais").value);
        var synthese anglais = parseFloat(document.getElementById("ynthese anglais").value);
        var moy anglais = (control anglais + oral anglais + synthese anglais + synthese anglais) /4;
        var score anglais = (moy anglais) *2;


        var control tari5 = parseFloat(document.getElementById("control tari5").value);
        var oral tari5 = parseFloat(document.getElementById("oral tari5").value);
        var synthese tari5 = parseFloat(document.getElementById("synthese tari5").value);
        var score tari5 = (oral tari5 + control tari5 + synthese tari5 + synthese tari5) /4;


        var control jo8rafiya = parseFloat(document.getElementById("control jo8rafiya").value);
        var oral jo8rafiya = parseFloat(document.getElementById("oral jo8rafiya").value);
        var synthese jo8rafiya = parseFloat(document.getElementById("synthese jo8rafiya").value);
        var score jo8rafiya = (control jo8rafiya + oral jo8rafiya + synthese jo8rafiya + synthese jo8rafiya) /4;


        var control islamiya = parseFloat(document.getElementById("control islamiya").value);
        var oral islamiya = parseFloat(document.getElementById("oral islamiya").value);
        var synthese islamiya = parseFloat(document.getElementById("synthese islamiya").value);
        var score islamiya = (oral islamiya + control islamiya + synthese islamiya + synthese islamiya) /4;


        var control madaniya = parseFloat(document.getElementById("control madaniya").value);
        var oral madaniya = parseFloat(document.getElementById("oral madaniya").value);
        var synthese madaniya = parseFloat(document.getElementById("synthese madaniya").value);
        var score madaniya = (oral madaniya + control madaniya + synthese madaniya + synthese madaniya);


        var control math lwl = parseFloat(document.getElementById("control math 1").value);
        var control math thani = parseFloat(document.getElementById("control math 2").value);
        var synthese math = parseFloat(document.getElementById("synthese math").value);
        var moy math = (control math lwl + control math thani + synthese math + synthese math) /4;
        var score math = (moy math) *4;



        var tp physique = parseFloat(document.getElementById("TP physique").value);
        var control physique = parseFloat(document.getElementById("control physique").value);
        var synthese physique = parseFloat(document.getElementById("synthese physique").value);
        var moy physique = (tp physique + control physique + synthese physique + synthese physique) /4;
        var score physique =(moy physique) *3;


        var project technique = parseFloat(document.getElementById("project technique").value);
        var control technique = parseFloat(document.getElementById("control technique").value);
        var synthese technique = parseFloat(document.getElementById("synthese technique").value);
        var moy technique = (project technique + control technique + synthese technique + synthese technique) /4;
        var score technique = (moy technique) *2;


        var Sport = parseFloat(document.getElementById("Sport").value);

        var average = (score info + score arabe + score francais + score anglais + score tari5 + score jo8rafiya + score islamiya + score madaniya + score math + score physique + score technique + Sport) / 23;
        document.getElementById("average").innerHTML = "mo3dlek : " + average;
      }
    </script>
   
